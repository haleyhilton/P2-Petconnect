<form>
  <div class="form-group">
    <label for="dog-name">Dog's Name: </label>
    <input class="form-input" type="text" id="dog-name" />
  </div>
  <div class="form-group">
    <label for="dog-age">Age: </label>
    <input class="form-input" type="text" id="dog-age" />
  </div>
  <div class="form-group">
    <label for="dog-breed">Breed type: </label>
    <input class="form-input" type="text" id="dog-breed" />
  </div>
  <div class="form-group">
    <label for="dog-size">Size: </label>
    <input id="small" class="dog-size form-input" type="radio" name="dog-size" value="small" />
    Small
    <input id="medium"  class="dog-size form-input" type="radio" name="dog-size" value="medium" />
    Medium
    <input id="large"  class="dog-size form-input" type="radio" name="dog-size" value="large" />
    Large
    <br />
  </div>
  <div class="form-group">
    <label for="dog-sex">Sex: </label>
    <input id="male" class="form-input" type="radio" name="dog-sex" value="male" />
    Male
    <input id="female" class="form-input" type="radio" name="dog-sex" value="female" />
    Female
    <br />
  </div>
  <div class="form-group">
    <label for="dog-color">Color: </label>
    <input class="form-input" type="text" id="dog-color" />
  </div>
    <div class="form-group">
    <label for="dog-description">Description: </label>
    <input class="form-input" type="text" id="dog-description" />
  </div>
  {{!-- <input type="file" name="image" /> --}}
  <input id="dog-submit" type="submit" />
</form>
<script src="public\js\newDog.js"></script>